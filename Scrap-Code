      <input id="question_1_1_1_1_1_1_1_1_1_1_1_1_1_1_2_1" name="group_25" type="radio" onClick="scrollSmoothToBottom()" />
      <label for="question_1_1_1_1_1_1_1_1_1_1_1_1_1_1_2_1">Yes!</label>
    </p>
